{
	"manifest_version": 2,
	"name": "ImageFilter",
	"namemsg": "imagefilter-extname",
	"version": "1.2",
	"descriptionmsg": "imagefilter-desc",
	"type": "parserhook",
	"author": [
		"[https://mediawiki.org/wiki/User:Nx Nx]",
		"Tim Starling"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:ImageFilter",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\ImageFilter\\Hooks",
			"services": [
				"UserOptionsManager",
				"PageProps",
				"LinkRenderer"
			]
		}
	},
	"Hooks": {
		"PageRenderingHash": "main",
		"ImageBeforeProduceHTML": "main",
		"GetPreferences": "main",
		"GetDoubleUnderscoreIDs": "main"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\ImageFilter\\": "includes/"
	},
	"AutoloadClasses": {
		"MediaWiki\\Extension\\ImageFilter\\Hooks": "includes/Hooks.php"
	},
	"MessagesDirs": {
		"ImageFilter": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"ImageFilterMagic": "ImageFilter.i18n.magic.php"
	},
	"callback": "MediaWiki\\Extension\\ImageFilter\\Hooks::setup"
}
